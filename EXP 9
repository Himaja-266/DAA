#include <stdio.h>

void sort(int arr[], int n) {
    for(int i = 0; i < n - 1; i++) {
        for(int j = 0; j < n - i - 1; j++) {
            if(arr[j] > arr[j + 1]) {
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
}

int binarySearch(int arr[], int n, int key) {
    int low = 0, high = n - 1;

    while(low <= high) {
        int mid = low + (high - low) / 2;

        if(arr[mid] == key)
            return mid;
        else if(arr[mid] < key)
            low = mid + 1;
        else
            high = mid - 1;
    }
    return -1;
}

int main() {
    int arr[] = {3,4,6,-9,10,8,9,30};
    int key = 10;
    int n = sizeof(arr) / sizeof(arr[0]);

    sort(arr, n);

    int result = binarySearch(arr, n, key);

    if(result != -1)
        printf("Element %d is found at position %d\n", key, result + 1);
    else
        printf("Element not found\n");

    return 0;
}
OUTPUT:Element 10 is found at position 5
