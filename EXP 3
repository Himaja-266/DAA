#include <stdio.h>

// Function to calculate sum of squares of distinct counts
int sumOfSquares(int nums[], int n) {
    int totalSum = 0;

    for (int i = 0; i < n; i++) {
        int freq[1001] = {0};   // Adjust size if needed
        int distinctCount = 0;

        for (int j = i; j < n; j++) {
            if (freq[nums[j]] == 0) {
                distinctCount++;
            }
            freq[nums[j]]++;

            totalSum += distinctCount * distinctCount;
        }
    }

    return totalSum;
}

int main() {
    int nums[] = {1, 2, 1};
    int n = sizeof(nums) / sizeof(nums[0]);

    printf("Output: %d\n", sumOfSquares(nums, n));

    return 0;
}
Output: 15
